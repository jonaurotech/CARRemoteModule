<UserControl x:Class="CAR.user_controls.sideMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CAR.user_controls"
             mc:Ignorable="d" 
             d:DesignHeight="640" d:DesignWidth="300">
   <DockPanel>
      <Grid DockPanel.Dock="Left">
         <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="4*"/>
         </Grid.ColumnDefinitions>
        

            <StackPanel  Orientation="Vertical" Grid.Column="0">
               <Button Name="aButton" Margin="0,0,0,50" MaxWidth="50" MinHeight="50"/>
               <Button Name="dButton" Margin="0,0,0,50" MaxWidth="50" MinHeight="50"/>
               <Button Name="saButton" Margin="0,0,0,50" MaxWidth="50" MinHeight="50"/>
               <Button Name="suButton" Margin="0,0,0,50" MaxWidth="50" MinHeight="50"/>
               <Button Name="apoButton" Margin="0,0,0,50" MaxWidth="50" MinHeight="50"/>
               <Button Name="cenButton" Margin="0,0,0,50" MaxWidth="50" MinHeight="50"/>
            </StackPanel>

        
      </Grid>
   </DockPanel>
</UserControl>
