<UserControl x:Class="CAR.user_controls.banner"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CAR.user_controls"
             mc:Ignorable="d" Width="960" Height="110">
   <DockPanel>
      <Grid DockPanel.Dock="Top" >
         <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
         </Grid.ColumnDefinitions>

         <Image Name="oneImage" Source="/CAR;component/resources/images/NewBanner.jpg" VerticalAlignment="Top" Stretch="None" Panel.ZIndex="-1" HorizontalAlignment="Left" Width="1325" Grid.ColumnSpan="2" Margin="0,0,-365,0"/>
       


      </Grid>
   </DockPanel>
</UserControl>
